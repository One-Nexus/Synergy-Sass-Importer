{
  "name": "@onenexus/sass-json-importer",
  "version": "1.0.0",
  "description": "A Node-Sass importer to allow for importing of JSON files",
  "main": "dist/sass-json-importer.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/One-Nexus/sass-json-importer.git"
  },
  "keywords": [
    "sass",
    "node-sass",
    "importer",
    "json"
  ],
  "scripts": {
