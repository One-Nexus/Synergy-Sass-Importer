  },
  "dependencies": {
    "is-there": "^4.0.0",
    "json5": "^1.0",
    "lodash": "^4.17"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "deep-extend": "^0.6.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.2",
    "node-sass": ">=3.5.3"
  }
}
